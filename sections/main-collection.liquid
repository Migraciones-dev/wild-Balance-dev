<script>
  document.addEventListener('DOMContentLoaded', function () {
  setTimeout(() => {
    const addToCartBtn = document.querySelectorAll('.t4s-pr-addtocart');
    addToCartBtn.forEach((button) => {
      button.addEventListener('click', () => {
        const productId = button.getAttribute('data-id');
        const variantId = button.getAttribute('data-variant-id');
        const quantity = 1;
        const productWrapper = button.closest('.t4s-product-wrapper');
        const productTitle = productWrapper.querySelector('.t4s-product-title a').innerText;
        const productPrice = productWrapper.querySelector('.t4s-product-price').innerText;

        if (productPrice.includes('Desde')) {
          setTimeout(() => {
            const newAddToCart = document.querySelector('.t4s-modal--is-active');
            const newItems = document.querySelectorAll('.t4s-swatch__item');

            newAddToCart.classList.add('listener-attached');

            newAddToCart.addEventListener('click', () => {
              const items = [];

              newItems.forEach((item) => {
                const quantityInput = item.querySelector('.t4s-quantity-input');
                const quantity = quantityInput?.value;
                console.log('quantoity', quantity)
                if (quantity && quantity != 0) {
                  const price = item.querySelector('.priceAndQuantity-pdpBlock span')?.innerText;
                  const variantId = quantityInput.getAttribute('id').replace('input[', '').replace(']', '');

                  items.push({
                    item_id: productId,
                    item_name: productTitle,
                    item_brand: 'Wild balance',
                    item_category: '{{ collection.title | remove: "'" | remove: '"' }}',
                    item_variant: variantId,
                    currency: '{{ shop.currency }}',
                    price: price,
                    quantity: quantity
                  });
                }
              });

              dataLayer.push({ ecommerce: null });
              dataLayer.push({
                event: 'add_to_cart_collection_drawer',
                ecommerce: {
                  items: items
                }
              });
            },{ once: true });
          }, 1500);
        }
      });
    });
  }, 1000);
});

</script>
