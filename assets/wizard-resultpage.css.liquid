  }

  .freezerSpace {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    align-self: center;
    width: 100%;
    margin-top: 20px;
  }

  .freezerSpace .contentFreezerSpace {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    align-items: center;
    width: 100%;
    gap: 20px;
  }

  .contentFreezerSpace h3 {
    font-size: 20px;
    font-weight: 700;
  }

  .contentFreezerSpace .articleInfoFrequently {
    width: clamp(130px, 46%, 400px);
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    padding: 16px 25px;
    gap: 10px;
    background-color: #fff;
    border-radius: 20px;
    border: 2px solid var(--lightGreen-css);
  }

  .contentFreezerSpace .articleInfoFrequently:hover {
    background-color: var(--lightGreen-css);
    border: 2px solid var(--WildBalanceGreen-css);
    cursor: pointer;
  }

  .contentFreezerSpace .articleInfoFrequently:active,
  .contentFreezerSpace .articleInfoFrequently.active {
    background-color: var(--lightGreen-css);
    border: 2px solid var(--WildBalanceGreen-css);
  }

  .contentFreezerSpace .articleInfoFrequently:visited {
    background-color: var(--lightGreen-css);
    border: 2px solid var(--WildBalanceGreen-css);
  }

  .imgAndLabelInfo {
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 100%;
    justify-content: space-between;
    height: auto;
  }

  .imgAndLabelInfo div:nth-child(1) {
    width: 80px;
  }

  .imgAndLabelInfo div:nth-child(2) {
    width: auto;
    display: flex;
    flex-direction: column;
    align-items: flex-end;
    justify-content: flex-start;
  }

  .freezerSpaceSubtitle {
    text-align: center;
  }

  .freezerSpaceSubtitle:after {
    content:url('https://cdn.shopify.com/s/files/1/0713/9898/1933/files/wink_1.png?v=1713513484');
  }

  .articleInfoFrequently .textInfo {
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    flex-wrap:wrap;
  }

  .articleInfoFrequently .textInfo div {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: flex-start;
    gap: 5px;
  }

  .xselling.frequently .wizard-variant-results{
    padding:initial;
  }

  .textInfo .label.select,
  .textInfo .label.selected{
    line-height:30px;
