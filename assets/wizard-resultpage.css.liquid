    border-radius: 20px;
  }

  .headingPackName {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    gap: 10px;
  }

  .infoContainerPack ul {
    padding: 0;
    margin: 0;
  }

  .infoContainerPack li {
    list-style: none;
    list-style: none;
    display: flex;
    justify-content: flex-start;
    align-items: flex-start;
    font-size: 16px;
  }

  .infoContainerPack li:before {
    content: url('https://cdn.shopify.com/s/files/1/0713/9898/1933/files/check_6.png?v=1713169070');
